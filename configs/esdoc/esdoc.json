{
  "source": "./src",
  "destination": "./build/docs",
  "includes": [".*\\.js", ".*\\.jsx"],
  "package": "./package.json",
  "index": "./README.md",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "lint": { "enable": true },
        "coverage": { "enable": true }
      }
    },
    { "name": "esdoc-jsx-plugin" },
    { "name": "esdoc-node" },
    { "name": "esdoc-ecmascript-proposal-plugin", "option": { "all": true } }
  ]
}
