{
  "source": "src",
  "destination": "./dist/docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "lint": { "enable": true },
        "coverage": { "enable": true }
      }
    },
    {
      "name": "esdoc-jsx-plugin"
    },
    {
      "name": "esdoc-node"
    }
  ]
}
