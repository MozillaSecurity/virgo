{
  "name": "virgo",
  "version": "0.1.0",
  "description": "Virgo - A crowdfuzz approach for the desktop.",
  "keywords": [
    "cluster",
    "fuzzing",
    "utility",
    "bounty",
    "bugs"
  ],
  "homepage": "https://github.com/mozillasecurity/virgo",
  "bugs": {
    "url": "https://github.com/mozillasecurity/virgo/issues"
  },
  "license": "MPL-2.0",
  "author": "Christoph Diehl <cdiehl@mozilla.com>",
  "repository": "github:mozillasecurity/virgo",
  "private": true,
  "engines": {
    "node": ">=11"
  },
  "main": "dist/main/main.js",
  "scripts": {
    "\n# Build": "",
    "start": "cross-env NODE_ENV=development run-p -r parcel:launch electron:launch",
    "dist": "cross-env NODE_ENV=production run-s parcel:build electron:build",
    "\n# Intermediate": "",
    "dist-universal": "run-s parcel:build electron:build:universal",
    "electron:launch": "run-s parcel:main electron:start",
    "electron:start": "cross-env ELECTRON_START_URL=http://localhost:3000 electron --enable-logging --remote-debugging-port=9223 .",
    "electron:build": "node configs/electron/builder.js macos linux64",
    "parcel:launch": "node configs/parcel/bundler.js",
    "parcel:build": "run-s parcel:main parcel:renderer",
    "parcel:main": "parcel build src/main/main.js --out-dir dist/main --out-file=main --target=electron",
    "parcel:renderer": "parcel build src/renderer/index.html --log-level 3 --no-cache --public-url ./ --out-dir dist/renderer/production --target=electron",
    "\n# Debug": "",
    "debug:electron": "electron --inspect-brk=9222 --enable-logging --remote-debugging-port=9223 .",
    "debug:launch": "run-p parcel:main parcel:launch debug:electron",
    "\n# Test": "",
    "test": "cross-env run-s parcel:build test:jest",
    "test:jest": "cross-env CI=true jest --config configs/jest/jest.config.js --colors",
    "test:update": "npm run test:jest -u",
    "test:coverage": "npm run test:jest -- --coverage",
    "\n# Lint": "",
    "eslint": "eslint --config configs/eslint/eslintrc.js --ignore-path configs/eslint/eslintignore",
    "stylelint": "stylelint --config configs/stylelint/stylelint.config.js --formatter=verbose",
    "lint": "run-s -c typecheck lint:js lint:css",
    "lint:js": "npm run eslint -- --fix 'src/**/*.{js,jsx}'",
    "lint:css": "npm run stylelint -- 'src/**/*.{css,scss}' --fix",
    "\n# Format": "",
    "prettier": "prettier --config prettier.config.js --ignore-path configs/prettier/prettierignore",
    "format": "run-s -c format:js format:css",
    "format:js": "npm run prettier -- --write 'src/**/*.{js,jsx}'",
    "format:css": "npm run prettier -- --write './src/**/*.{css,scss}'",
    "\n# Documentation": "",
    "docs": "esdoc -c configs/esdoc/esdoc.json",
    "\n# Flow": "",
    "typecheck": "flow check --quiet --color=always --flowconfig-name configs/flow/flowconfig",
    "typecheck:init": "flow-typed install --overwrite",
    "typecheck:update": "flow-typed update",
    "typecheck:upgrade": "flow-upgrade",
    "typecheck:coverage": "flow-coverage-report --config configs/flow/flowcoverage.json",
    "\n# Packages": "",
    "updates": "ncu -t"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "npm run eslint -- --fix",
        "git add"
      ],
      "*.{css,scss}": [
        "npm run stylelint -- --fix",
        "git add"
      ],
      "*.{json,md}": [
        "npm run prettier -- --write",
        "git add"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@jest-runner/electron": "1.1.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "cross-env": "^5.2.0",
    "devtron": "^1.4.0",
    "electron": "^5.0.0",
    "electron-builder": "^20.39.0",
    "electron-devtools-installer": "^2.2.4",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-node": "^1.0.4",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-flowtype": "^3.7.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.4",
    "flow-bin": "^0.98.0",
    "flow-coverage-report": "^0.6.1",
    "flow-typed": "^2.5.1",
    "husky": "^2.2.0",
    "jest": "^24.7.1",
    "jest-config": "^24.7.1",
    "lint-staged": "^8.1.5",
    "npm-check-updates": "^3.1.8",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.0",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.8.6",
    "regenerator-runtime": "^0.13.2",
    "spectron": "^5.0.0",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-prettier": "^1.0.6"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "app-root-path": "^2.2.1",
    "axios": "^0.18.0",
    "dockerode": "^2.5.8",
    "electron-log": "^3.0.5",
    "electron-store": "^3.2.0",
    "electron-updater": "^4.0.6",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "react-redux": "^7.0.3",
    "styled-components": "^4.2.0"
  }
}
